<% form_for @channel, :url => channel_subscriptions_path, :html => {:method => :post} do |form| %>  
  <div id="audience">
    <% fields_for Audience.new do |audience| %>
      <% unless audience.object.is_a?(Group) %>
        <%= render 'audiences/fields', :form => audience, :group_form => form %>
      <% end %>
    <% end %>
  </div>
  <%= form.submit 'Add' %>
<% end %>