

<% if current_user.role_memberships.recent.any? %>
  <h4>Recently Approved Roles</h4>
  <ul class="recent_role_approvals">
    <% current_user.role_memberships.not_public_roles.recent.each do |recent| %>
      <li class="approval"><%=h recent.jurisdiction.name %> : <%=h recent.role.name %></li>
    <% end %>
  </ul>
<% end %>