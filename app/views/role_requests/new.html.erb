<h1>New role_request</h1>

<% form_for(@role_request) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :requester_id %><br />
    <%= f.text_field :requester_id %>
  </p>
  <p>
    <%= f.label :role_id %><br />
    <%= f.text_field :role_id %>
  </p>
  <p>
    <%= f.label :approver_id %><br />
    <%= f.text_field :approver_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', role_requests_path %>